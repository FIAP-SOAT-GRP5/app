{
	"containerDefinitions": [
		{
			"essential": true,
			"name": "app_container",
			"portMappings": [
				{
					"hostPort": 3000,
					"containerPort": 3000,
					"protocol": "tcp"
				}
			]
		}
	],
	"memory": "512",
	"cpu": "256",
	"executionRoleArn": "arn:aws:iam::947025227707:role/role_ecs",
	"family": "fargate-task-definition",
	"networkMode": "awsvpc",
	"runtimePlatform": {
		"operatingSystemFamily": "LINUX"
	},
	"requiresCompatibilities": [
		"FARGATE"
	]
}